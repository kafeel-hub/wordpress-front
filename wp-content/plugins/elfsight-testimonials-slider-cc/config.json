  "settings": {
    "tabs": [
      {
        "id": "layout",
        "name": "Layout",
        "active": true
      },
      {
        "id": "content",
        "name": "Content"
      },
      {
        "id": "style",
        "name": "Style"
      },
      {
        "id": "advanced",
        "name": "Advanced"
      }
    ],
    "properties": [
      {
        "id": "template",
        "name": "Testimonial Template",
        "tab": "layout",
        "type": "select-visual",
        "defaultValue": "tiledClassic",
        "select": {
          "options": [
            {
              "value": "tiledClassic",
              "name": "Tiled Classic",
              "img": "data:image/png;base64,"
            },
            {
              "value": "tiledBalloon",
              "name": "Tiled Balloon",
              "img": "data:image/png;base64,"
            },
            {
              "value": "tiledPostcard",
              "name": "Tiled Postcard",
              "img": "data:image/png;base64,"
            },
            {
              "value": "singleClassic",
              "name": "Single Classic",
              "img": "data:image/png;base64,"
            },
            {
              "value": "singleSpotlight",
              "name": "Single Spotlight",
              "img": "data:image/png;base64,"
            },
            {
              "value": "singlePostcard",
              "name": "Single Postcard",
              "img": "data:image/png;base64,"
            }
          ]
        },
        "description": "Select one of the 6 predefined templates for the testimonials in your slider. Tiled templates allow you to display several testimonials at ones, depending on the size of your slider. Single templates display individual testimonials. Perfect choice for cases when you need to focus on each individual story."
      },
      {
        "id": "layout",
        "name": "Layout",
        "tab": "layout",
        "type": "select-visual",
        "defaultValue": "slider",
        "select": {
          "options": [
            {
              "value": "slider",
              "name": "Slider",
              "img": "data:image/png;base64,"
            },
            {
              "value": "grid",
              "name": "Grid",
              "img": "data:image/png;base64,"
            }
          ]
        },
        "description": "Choose Slider layout to display testimonials in a feed as part of your page's content, or Grid layout to display them on a separate page or individually."
      },
      {
        "id": "layoutSlider",
        "name": "Slider Settings",
        "tab": "layout",
        "type": "subgroup",
        "subgroup": {
          "properties": [
            {
              "id": "sliderArrows",
              "name": "Show navigation arrows",
              "type": "toggle",
              "defaultValue": true,
              "description": "Activate/Deactivate arrows in the slider."
            },
            {
              "id": "sliderDrag",
              "name": "Drag to navigate",
              "type": "toggle",
              "defaultValue": true,
              "description": "Activate/Deactivate drag in the slider."
            },
            {
              "id": "sliderSpeed",
              "name": "Slide switch speed (sec)",
              "type": "number",
              "defaultValue": 0.6,
              "description": "Animation speed of slide switching (sec)."
            },
            {
              "id": "sliderAutoplay",
              "name": "Autoplay speed (sec)",
              "type": "number",
              "defaultValue": 0,
              "description": "Autoplay in the slider (in seconds). If this option is set to \"0\" autoplay is turned off."
            },
            {
              "id": "sliderPaginationType",
              "name": "Pagination type",
              "type": "select-inline",
              "selectInline": {
                "options": [
                  {
                    "value": "classic",
                    "name": "Classic"
                  },
                  {
                    "value": "none",
                    "name": "None"
                  }
                ]
              },
              "defaultValue": "classic",
              "description": "Type of pagination in the slider."
            }
          ]
